journal = {Nuclear Engineering and Technology},
keywords = {Corrosion,Dimensional changes,Failure mechanisms,High burnup,LOCA,Normal operation,Nuclear fuel,PCI,PCMI,RIA,Zirconium alloys},
number = {1},
