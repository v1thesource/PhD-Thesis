title = {{High burnup fuel issues}},
volume = {40},
year = {2008}
}
@article{Zarkadoula2014,
abstract = {Zirconia is viewed as a material of exceptional resistance to$\$namorphization by radiation damage, and consequently proposed as a$\$ncandidate to immobilize nuclear waste and serve as an inert nuclear fuel$\$nmatrix. Here, we perform molecular dynamics simulations of radiation$\$ndamage in zirconia in the range of 0.1-0.5 MeV energies with account of$\$nelectronic energy losses. We find that the lack of amorphizability$\$nco-exists with a large number of point defects and their clusters.$\$nThese, importantly, are largely isolated from each other and therefore$\$nrepresent a dilute damage that does not result in the loss of long-range$\$nstructural coherence and amorphization. We document the nature of these$\$ndefects in detail, including their sizes, distribution, and morphology,$\$nand discuss practical implications of using zirconia in intense$\$nradiation environments. (C) 2014 AIP Publishing LLC.},
author = {Zarkadoula, E and Devanathan, R and Weber, W J and Seaton, M A and Todorov, I T and Nordlund, K and Dove, M T and Trachenko, K},
