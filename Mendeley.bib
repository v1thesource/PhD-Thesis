@article{Ewing2004,
abstract = {The immobilization, safe storage, and disposal of plutonium and other actinides by the use of pyrochlore structure-types is discussed. Pyrochlore has the ability to accomodate a wide variety of chemistries and some compositions such as the titanates and zirconates because of its durable nature under expected repository conditions. The radiation damage and dose-rate effects in pyrochlores that are proposed for the immobilization of actinides have been concluded fron using both short-lived actinides for doping experiments and ion-beam irradiations along with studies of U- and Th-bearing minerals. Amorphization under all repository conditions results from the alpha-decay damage in titanate-based pyrochlore compounds.},
