isbn = {9026234422},
journal = {World Journal of Stomatology},
keywords = {although all zirconia is,biocompatibility,chemically similar,core tip,from manufacturer to manu-,mechanical properties,ping,porcelain chip-,the,ultimate product can vary,zirconia},
number = {3},
pages = {40--47},
title = {{Basic properties and types of zirconia: An overview}},
volume = {2},
