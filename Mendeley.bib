@article{Thomas1979,
abstract = {The Studsvik Inter Ramp Project is an experimental power ramping project internationally sponsored by 14 different organizations and managed by Studsvik Energiteknik AB of Sweden. The objective of the project is to investigate failure propensity and characteristics of 20 unpressurized fuel rods of typical 8 × 8 BWR design when subjected to overpower ramp operation after having completed prior extended irradiations (9 to 23 GWd/MTU accumulated peak burnups) containing idealized cyclic power-level variations. All irradiations were performed in the Studsvik R2 reactor in conditions simulating BWR coolant temperatures and pressures. Fuel rod design variables included fuel-cladding gap size, initial cladding heat treatment, and fuel density. The power ramping tests produced 11 failures (leaking rods) out of the 20 total in highly-consistent patterns strongly linking ramping performance to integral effects created during the preceding extended irradiations - i.e., the results indicated power shock was more dominant than peak ramp power. Permanent cladding deformation was small (generally 0.05-0.1) and burnup dependence of failure appeared negligible for the range studied. {\textcopyright} 1979.},
author = {Thomas, G. R.},
doi = {10.1016/0022-3115(79)90557-9},
issn = {00223115},
journal = {Journal of Nuclear Materials},
