@incollection{Michel2012,
abstract = {This chapter focuses on the modeling of pellet-cladding interaction (PCI) in pressurized water reactor under nominal and transient loading conditions. Scientific results proposed in this chapter have already been published and are mainly based on an important experimental and research cooperative program between EDF, AREVA, and the Atomic Energy Commission CEA which was initiated to get a better understanding of the mechanisms possibly leading to PCI failure, as well as to qualify a PCI-resistant rod design.In the first part, the phenomenological aspects of PCI are detailed with a brief bibliographic review of modeling approaches developed in France and abroad.In the second part, experimental approach of PCI is discussed, with the objective of presenting an overall vision of the knowledge coming from postirradiation analyses of industrial fuel rods and also power ramp test results. Due to the large number of nonlinear phenomena involved in PCI behavior, it is however very difficult to get a full understanding of the mechanisms at hand from experimental results only. A detailed modeling approach is necessary. Among the other motivations which have led to the development of PCI modeling, we can list: the difficulty and the cost of instrumented PCI experiments, the need for tools to develop representative laboratory tests, and also the ambition to use numerical simulation to reduce the development and qualification periods of a product and to optimize fuel rod design.In the third part of this chapter, PCI modeling and numerical simulation are presented via the description of the thermal, mechanical, and physical problems that need to be solved and the knowledge linked to each of the contributing mechanisms. Attention is focused on the multidimensional PCI modeling environment PLEIADES codeveloped by the Atomic Energy Commission CEA and EDF in the framework of the tripartite cooperation with AREVA. Improvements brought about by the use of a multidimensional PCI modeling are detailed, particularly with regard to the simulation of the coupling between gaseous swelling and mechanical behavior of the fuel pellet.In the last part, the contribution of modeling to a better understanding of PCI is detailed with simulation results. For each elementary mechanism, at the fuel element scale, a discussion is proposed on the following items: comparison between computation and available measurements, first-order parameters based on sensitivity analyses, the type of modeling needed, and the contribution of 1D, 2D, or 3D approaches. Through these results, it appears that PCI simulation provides a quantitative assessment of the competition between nonlinear phenomena taking place in the fuel element under irradiation. Finally, a PCI failure risk criterion based on a theoretical approach is proposed, its validation and limits are discussed. ?? 2012 Elsevier Ltd All rights reserved.},
author = {Michel, B and Sercombe, J and Nonon, C and Fandeur, O},
booktitle = {Comprehensive Nuclear Materials},
doi = {10.1016/B978-0-08-056033-5.00074-4},
isbn = {9780080560335},
keywords = {Finite element computation,Fuel performance code,Multiphysic algorithm,Pellet-cladding interaction},
pages = {677--712},
title = {{Modeling of pellet cladding interaction}},
volume = {3},
year = {2012}
}
@phdthesis{Xia2010,
abstract = {Interatomic potential and quantum mechanical simulation techniques have been applied to both the bulk and surfaces of yttrium-stabilized cubic zirconia (YSZ) with special reference to its role in partial oxidation reaction at high temperature. The stabilities of pure ZrO2 phases and surfaces have been examined. The bulk structures of low pressure phases are reproduced with the observed order of stability: monoclinic >tetragonal >cubic zirconia. The relative stability of plane cubic ZrO2 surfaces is predicted to be (111)c >(110)c >(100)c >(310)c. In addition, the stability of topological surfaces is found to be reduced with the coordination number of Zr ions at the topological sites in the order: plane >step >kink >corner. The dispersion of defects in YSZ systems has been studied, considering both the dopant content and surface segregation. In the bulk, the yttrium dopants tend to form a pair with two yttrium atoms close to each other and preferentially occupying the 1st or 2nd nearest neighbour (NN) sites to the compensating oxygen vacancy. At the surface, yttrium segregates to the top layers (up to 4-5 {\AA}) of the dominant (111) surface of YSZ. The composition of the outermost surface of YSZ is predicted to be independent of Y bulk concentration and reach a maximum Y/Zr ratio of 1:1. In relation to catalytic oxidation, the interaction between oxygen molecules and the (111) surfaces of pure c-ZrO2 and YSZ have been investigated using quantum- mechanical DFT-GGA methods. The adsorption states of oxygen and the pathways for dissociation have been indentified on the plane and stepped (111) surfaces. In general, the creation of oxygen vacancies by yttrium doping provides an active site for oxygen adsorption. In addition, the low-coordinated Zr cations on the YSZ surfaces can attract strongly reduced oxygen species and the most stable adsorption state of oxygen is adopted to achieve a higher bond saturation of the neighbouring Zr site.},
author = {Xia, X.},
booktitle = {Ph.D thesis},
school = {University College London},
title = {{Computational Modelling Study of Yttria-stabilized Zirconia}},
year = {2010}
}
