title = {{Amount of iodine responsible for I-SCC of Zircaloy-4 in PCI-conditions: recoil-implanted and thermally released iodine}},
volume = {186},
year = {1998}
}
@article{Cooper2014c,
abstract = {A many-body potential model for the description of actinide oxide systems, which is robust at high temperatures, is reported for the first time. The embedded atom method is used to describe many-body interactions ensuring good reproduction of a range of thermophysical properties (lattice parameter, bulk modulus, enthalpy and specific heat) between 300 and 3000 K for AmO2, CeO2, CmO2, NpO2, ThO2, PuO2 and UO2. Additionally, the model predicts a melting point for UO2 between 3000 and 3100 K, in close agreement with experiment. Oxygen-oxygen interactions are fixed across the actinide oxide series because it facilitates the modelling of oxide solid solutions. The new potential is also used to predict the energies of Schottky and Frenkel pair disorder processes.},
