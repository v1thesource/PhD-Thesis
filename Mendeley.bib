}
@article{Haines1997,
abstract = {The crystal structures of the cotunnite-type phases (space group, Pnam, Z = 4) of pure zirconia and hafnia prepared under high-temperature, high-pressure conditions in a multianvil device were refined by time-of-flight neutron powder diffraction. The structures of both compounds are very similar and the nine polyhedral metal-oxygen distances range from 2.133(1) to 2.546(1) {\AA} in ZrO2 and from 2.121(1) to 2.535(2) {\AA} in HfO2. The Raman spectra of both phases resemble one another strongly and are consistent with the cotunnite-type structure. These results confirm that ZrO2 and HfO2 undergo transitions to the same phase at high pressure.},
